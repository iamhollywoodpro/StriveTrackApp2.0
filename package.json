{
  "name": "strivetrack-2.1",
  "version": "2.1.0",
  "description": "StriveTrack 2.1 - Pure Cloudflare Architecture with Parcel",
  "source": "src/index.html",
  "scripts": {
    "dev": "parcel src/index.html --port 3001 --host 0.0.0.0",
    "build": "parcel build src/index.html --public-url ./",
    "build:production": "npm run clean && parcel build src/index.html --public-url ./ --no-source-maps",
    "clean": "rm -rf dist .parcel-cache",
    "deploy": "node deploy.js",
    "deploy:quick": "npm run build:production && npx wrangler pages deploy dist",
    "deploy:worker": "node deploy-worker.js",
    "deploy:full": "node preflight-check.js && node deploy.js && node deploy-worker.js",
    "preflight": "node preflight-check.js",
    "preview": "npx wrangler pages dev dist --port 3001"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "parcel": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}